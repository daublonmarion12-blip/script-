local scriptURL = "https://raw.githubusercontent.com/daublonmarion12-blip/script-/main/MM2"

local success, result = pcall(function()
    return loadstring(game:HttpGet(scriptURL))()
end)

if not success then
    warn("Erreur lors du chargement du script : " .. tostring(result))
end
